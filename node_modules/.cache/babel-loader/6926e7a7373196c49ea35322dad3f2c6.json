{"ast":null,"code":"import{FurnitureType}from'@nitrots/nitro-renderer';import{GetSessionDataManager}from'./GetSessionDataManager';export function GetFurnitureDataForProductOffer(offer){if(!offer)return null;var furniData=null;switch(offer.productType.toUpperCase()){case FurnitureType.FLOOR:furniData=GetSessionDataManager().getFloorItemData(offer.furniClassId);break;case FurnitureType.WALL:furniData=GetSessionDataManager().getWallItemData(offer.furniClassId);break;}return furniData;}","map":null,"metadata":{},"sourceType":"module"}